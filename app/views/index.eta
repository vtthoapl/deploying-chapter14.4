<h1>Chat</h1>
<h2>Add an message</h2>
<form method="POST">
  Sender: <input type="text" name="sender"/><br/><br/>
  Message: <input type="text" name="message"/><br/><br/>
  <input type="submit" value="Submit!"/>
</form>
<h2>Messages</h2>
<% it.messages.forEach((message) => { %>
<p><%= message.sender%>: <%= message.message%></p>
<% }); %>
